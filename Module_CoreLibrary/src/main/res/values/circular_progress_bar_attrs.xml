<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="MissingDefaultResource,ExtraTranslation">
    <declare-styleable name="CircularProgressBar">
        <attr name="newsCpbStyle" format="reference"/>
        <attr name="newsCpb_color" format="color"/>
        <attr name="newsCpb_colors" format="reference"/>
        <attr name="newsCpb_stroke_width" format="dimension"/>
        <attr name="newsCpb_min_sweep_angle" format="integer"/>
        <attr name="newsCpb_max_sweep_angle" format="integer"/>
        <attr name="newsCpb_sweep_speed" format="float"/>
        <attr name="newsCpb_rotation_speed" format="float"/>
    </declare-styleable>


    <color name="cpb_default_color">#a7d3f6</color>

    <dimen name="cpb_default_stroke_width">4dp</dimen>

    <integer name="cpb_default_min_sweep_angle">20</integer>
    <integer name="cpb_default_max_sweep_angle">300</integer>

    <item name="cpb_default_sweep_speed" format="float" type="string">1</item>
    <item name="cpb_default_rotation_speed" format="float" type="string">1</item>

</resources>